#include<iostream>
#include<string.h>
#include<vector>
using namespace std;

vector<string> input_string(vector<string> a)
{
    string s=" ";
    while(s[0]!='0')
    {
        cin>>s;
        if(s[0]!='0')
            a.push_back(s);
    }
    return a;
}
int arr[26];
void assign_null()
{
    for(int i=0; i<26; i++){
        arr[i] = -1;
    }
}
vector<string> str;
int search_string(string s)
{
    int found = -1;
    for(int i=0; i<str.size(); i+=2)
        if(str[i] == s)
        {
            return stoi(str[i+1]);
        }
    return found;
}
int alphaCode(string s)
{
        int saved = search_string(s);
        if(saved != -1)
        {
            cout<<s<<" "<<saved<<endl;
            return saved;
        }
        if(s.length()<=1)return 1;
        int num1=0, num2=0;
        /*int temp_1 = (int)(s[0]-'0');
        if(arr[temp_1-1] != -1){cout<<"A"<<endl;
            num1 = arr[temp_1-1];}
        else{
        */
        num1 = alphaCode(s.substr(1,s.length()-1));
        //arr[(int)(s[0]-'0')-1] = num1;}
        int temp = stoi(s.substr(0,2));
        if(temp<27)
            /*if(arr[temp-1] != -1){cout<<"B"<<endl;
            num2 = arr[temp-1];}
            else{
            */
            num2 = alphaCode(s.substr(2,s.length()-2));
            //arr[temp-1] = num2;}
        str.push_back(s);
        str.push_back(to_string(num1+num2));
        return num1+num2;
}
